{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "react/prop-types": 0,
    "no-else-return": 0,
    "react/no-multi-comp": 0,
    "react/prefer-es6-class": 0,
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "react/require-render-return": 0,
    "max-len": [2, 150, 2]
  }
}
